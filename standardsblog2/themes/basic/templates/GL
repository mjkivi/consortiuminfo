UPDATE string in mySQL

UPDATE node__field_teaser 
SET field_teaser_value = REPLACE(field_teaser_value, '%http://www.consortiuminfo.org/standardsblog/images/library/image/%', '/consorD8/sites/default/files/inline-images/') 
WHERE field_teaser_value LIKE '%http://www.consortiuminfo.org/standardsblog/images/library/image/%' 
AND `entity_id` = 697;



